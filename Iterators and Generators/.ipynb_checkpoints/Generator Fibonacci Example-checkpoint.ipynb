    "def fibonacci():\n",
    "\n",
    "    a = 1\n",
    "    b = 1\n",
    "    yield a\n",
    "    yield b\n",
    "\n",
    "    while True:\n",
    "\n",
    "        a,b = b, a+b\n",
    "\n",
    "        yield b"
   ]
